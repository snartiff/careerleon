<div class="content">
  <div class="index link">
    <%= link_to "new career goal", new_goal_path %>
  </div>
  <div class="goal list">
    <h1>MY CAREER GOALS</h1>
    <ul>
      <% counter = 0 %>
      <% @goals.each do |goal| %>
      <% counter += 1 %>
        <% unless current_user.nil? %>
        <% end %>
         <li class="goal"><%="#{counter}.) "%><%= link_to("#{goal.title}", goal) %></li>
      <% end %>
    </ul>
  </div>
</div>
